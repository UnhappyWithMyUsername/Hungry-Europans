<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <!-- MEAT -->
    <!--Moloch-->
    <Item hideinmenus="false" name="Plate of Moloch Sushi" identifier="he-molochsushi" variantof="he-poisoneffectpreset_regular" health="100" category="Misc" subcategory="Food - Moloch" maxstacksize="1" maxstacksizecharacterinventory="20" cargocontaineridentifier="metalcrate" description="" Tags="smallitem,fooditem,gourmetfood,petfood1,petfood2,petfood3" useinhealthinterface="true" scale="0.5" impactsoundtag="impact_softitem" isshootable="True" requireaimtouse="True">
      <PreferredContainer secondary="abandonedmedcab" spawnprobability="0.02" />
      <PreferredContainer secondary="abandonedcrewcab" minamount="0" maxamount="1" spawnprobability="0.02" />
      <PreferredContainer secondary="crewcab" amount="1" spawnprobability="0.0" notcampaign="True"/>
      <PreferredContainer primary="kitchen" secondary="table,crewcab"/>
      <Sprite texture="%ModDir:2954998725%/Content/Items/Food/FoodAndMeat.png" sourcerect="582,30,68,29" depth="0.6" origin="0.5,0.9" />
      <InventoryIcon texture="%ModDir:2954998725%/Content/Items/Food/FoodAndMeat.png" sourcerect="582,30,68,29" origin="0.5,0.5" />
      <Fabricate suitablefabricators="cuttingboard" requiredtime="16" requiresrecipe="true" amount="1" displayname="molochfooditem">
        <RequiredSkill identifier="cooking" level="40" />
        <RequiredItem identifier="he-rawmolochmeat" amount="1" MinCondition="0.5" MaxCondition="1.0" UseCondition="False" />
        <RequiredItem identifier="he-nori" amount="8" />
        <RequiredItem identifier="he-vinegar" UseCondition="True" MinCondition="0.1"/>
        <RequiredItem identifier="he-salt" UseCondition="True" MinCondition="0.05"/>
        <RequiredItem identifier="he-emptyplate" amount="1" />
      </Fabricate>
      <Deconstruct time="1" requireddeconstructor="deconstructor"/>
      <Body width="60" radius="1" density="11" />
      <SuitableTreatment type="hunger" suitability="35" />
      <Holdable canbepicked="true" canBeCombined="false" removeOnCombined="false" slots="RightHand+LeftHand,Any" holdangle="0" holdpos="36,-20" handle1="0,-4" handle2="0,-4" aimable="false" msg="ItemMsgPickUpSelect">
        <!-- EVERYTHING RELATED TO POISONING FOOD IS HANDLED VIA THE POISON EFFECT PRESET ITEMS DEFINED IN - variantof="" AND THEIR CODE IS LOCATED IN - "/Content/Items/#Presets/#ItemPresets.xml" -->
        
        <!--////////// HEALTH MENU USAGE [BOT] \\\\\\\\\\-->
        <!-- allows health menu usage on bots as long as they are not in water -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-12.5" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Affliction identifier="he-damageresistance" amount="80.0" />
          <Affliction identifier="he-stuffed" amount="10.0" />
          <ReduceAffliction type="damage" amount="1.75" />
          <ReduceAffliction type="hunger" amount="2.5" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <ReduceAffliction identifier="mood" amount="2.5" />
        </StatusEffect>

        <!--////////// HEALTH MENU USAGE [PLAYER] \\\\\\\\\\-->
        <!-- only allows health menu usage for players who are not in water and are unconcious/stunned -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-12.5" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Affliction identifier="he-damageresistance" amount="80.0" />
          <Affliction identifier="he-stuffed" amount="10.0" />
          <ReduceAffliction type="damage" amount="1.75" />
          <ReduceAffliction type="hunger" amount="2.5" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <ReduceAffliction identifier="mood" amount="2.5" />
        </StatusEffect>

        <!--////////// REGULAR EATING \\\\\\\\\\-->
        <!-- Use the item when it's held and the fork in the other hand is used (indicated by applying a "usingfork" tag on the character which this item checks for) -->
        <StatusEffect type="OnSecondaryUse" target="This" Condition="-12.5" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional HasStatusTag="neq emptyhand" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="This" handle2="-30,60" setvalue="true" duration="0.5" stackable="false" comparison="And">
          <Conditional HasStatusTag="neq emptyhand" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="This" handle2="0,0" delay="0.5" duration="0.1" setvalue="true" stackable="false" comparison="And">
          <Conditional HasStatusTag="eq emptyhand" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" stackable="false" disabledeltatime="true" comparison="And">
          <Conditional HasStatusTag="neq emptyhand"/>
          <Conditional InWater="false" />
          <Affliction identifier="he-damageresistance" amount="80.0" />
          <Affliction identifier="he-stuffed" amount="10.0" />
          <ReduceAffliction type="damage" amount="1.75" />
          <Affliction identifier="spill" amount="1000" probability="0.0005" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/EatingSoft.ogg" loop="true" range="200" volume="5.0" selectionmode="Random" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" stackable="false" disabledeltatime="true" >
          <Conditional HasStatusTag="usingfork" />
          <Conditional HasStatusTag="neq emptyhand" />
          <Conditional InWater="false" />
          <Conditional hunger="gt 2.5" />
          <ReduceAffliction type="hunger" amount="2.5" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" stackable="false" disabledeltatime="true" >
          <Conditional HasStatusTag="usingfork" />
          <Conditional HasStatusTag="neq emptyhand" />
          <Conditional InWater="false" />
          <ReduceAffliction identifier="mood" amount="2.5" />
        </StatusEffect>
        <!-- Prevent using this item continuously by applying the tag "emptyhand" which prevents the StatusEffect that uses the item from triggering again, effectively only allowing it to trigger once per mouse-click -->
        <StatusEffect type="OnSecondaryUse" target="Character" tags="emptyhand" duration="0.5" stackable="false" />
        <StatusEffect type="OnBroken" target="This" disabledeltatime="true">
          <Remove />
          <SpawnItem identifier="he-dirtyplate" spawnposition="SameInventory" spawnifcantbecontained="true" />
        </StatusEffect>
      </Holdable>
      <AiTarget sightrange="1000" static="true" />
    </Item>

