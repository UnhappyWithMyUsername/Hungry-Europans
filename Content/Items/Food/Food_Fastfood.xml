    <Item hideinmenus="false" name="Nugget" identifier="he-cookednugget" health="10" category="Misc" subcategory="Food" maxstacksize="20" maxstacksizecharacterinventory="20" cargocontaineridentifier="metalcrate" description="" Tags="smallitem,fooditem,petfood1,petfood2,petfood3" useinhealthinterface="true" scale="0.5" impactsoundtag="impact_softitem" isshootable="True" requireaimtouse="True">
      <PreferredContainer secondary="wreckmedcab,abandonedmedcab" spawnprobability="0.02" />
      <PreferredContainer secondary="abandonedcrewcab" minamount="0" maxamount="1" spawnprobability="0.02" />
      <PreferredContainer secondary="crewcab" amount="1" spawnprobability="0.0" notcampaign="True"/>
      <PreferredContainer primary="kitchen" secondary="table,crewcab"/>
      <Sprite texture="%ModDir:2954998725%/Content/Items/Food/FoodAndMeat.png" sourcerect="225,455,17,11" depth="0.6" origin="0.5,0.5" />
      <InventoryIcon texture="%ModDir:2954998725%/Content/UI/InventoryIcons.png" sourcerect="191,0,55,38" origin="0.5,0.5" />
      <!-- 8 nuggets should about equal the values of 1 burger patty -->
      <Fabricate suitablefabricators="fryer" requiredtime="60" requiresrecipe="false" amount="16">
        <RequiredSkill identifier="cooking" level="15" />
        <RequiredItem identifier="he-rawnuggets" amount="1" MinCondition="0.5" MaxCondition="1.0" UseCondition="False" />
        <RequiredItem identifier="he-cookingoil" UseCondition="True" MinCondition="0.25"/>
      </Fabricate>
      <Deconstruct time="1" requireddeconstructor="deconstructor"/>
      <Body width="78" radius="5" density="11" />
      <SuitableTreatment type="hunger" suitability="35" />
      <Holdable canbepicked="true" canBeCombined="false" removeOnCombined="false" slots="Any,RightHand,LeftHand" handle1="5,-10" aimpos="20,12" holdangle="270" holdpos="25,30" aimangle="0" aimable="false" swingwhenaiming="true" swingamount="10,6" swingspeed="0.2" msg="ItemMsgPickUpSelect">
        <!--////////// HEALTH MENU USAGE [BOT] \\\\\\\\\\-->
        <!-- allows health menu usage on bots as long as they are not in water -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-10" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="2.5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Affliction identifier="he-stuffed" amount="1.125" />
          <ReduceAffliction type="damage" amount="0.15" />
          <ReduceAffliction type="hunger" amount="0.5" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="2.5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Conditional trait_likes_nuggets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>
        
        <!--////////// HEALTH MENU USAGE [PLAYER] \\\\\\\\\\-->
        <!-- only allows health menu usage for players who are not in water and are unconcious/stunned -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-10" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="2.5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Affliction identifier="he-stuffed" amount="1.125" />
          <ReduceAffliction type="damage" amount="0.15" />
          <ReduceAffliction type="hunger" amount="0.5" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="2.5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Conditional trait_likes_nuggets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>

        <!--////////// REGULAR EATING \\\\\\\\\\-->
        <StatusEffect type="OnUse" target="This" Condition="-4.0">
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Affliction identifier="he-stuffed" amount="1.125" />
          <ReduceAffliction type="damage" amount="0.15" />
          <Affliction identifier="spill" amount="1000" probability="0.0005" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/EatingSoft.ogg" loop="True" range="200" volume="5.0" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Conditional hunger="gt 0.5" />
          <ReduceAffliction type="hunger" amount="0.5" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="Character" comparison="And">
          <Conditional InWater="false" />
          <Conditional trait_likes_nuggets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>
        <StatusEffect type="OnBroken" target="This">
          <Remove />
        </StatusEffect>
      </Holdable>
