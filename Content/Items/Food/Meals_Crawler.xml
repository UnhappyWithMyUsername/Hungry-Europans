<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <!-- MEAT -->
    <!--Crawler-->
    <Item hideinmenus="false" name="Crawler Filet with Sea Weed" identifier="he-cookedcrawlerfilet" variantof="he-poisoneffectpreset_fork" health="100" category="Misc" subcategory="Food" maxstacksize="1" maxstacksizecharacterinventory="1" cargocontaineridentifier="metalcrate" description="" Tags="smallitem,fooditem,forkfood,foodplate,petfood1,petfood2,petfood3" useinhealthinterface="true" scale="0.5" impactsoundtag="impact_bottle_glass" isshootable="false" requireaimtouse="false">
      <PreferredContainer secondary="wreckmedcab,abandonedmedcab" spawnprobability="0.02" />
      <PreferredContainer secondary="abandonedcrewcab" minamount="0" maxamount="1" spawnprobability="0.02" />
      <PreferredContainer primary="table" secondary="kitchen,crewcab"/>
      <Sprite texture="%ModDir:2954998725%/Content/Items/Food/FoodAndMeat.png" sourcerect="651,30,68,29" depth="0.6" origin="0.5,0.9" />
      <InventoryIcon texture="%ModDir:2954998725%/Content/Items/Food/FoodAndMeat.png" sourcerect="651,30,68,29" origin="0.5,0.5" />
      <Fabricate suitablefabricators="cuttingboard" requiredtime="10" requiresrecipe="false" amount="1">
        <RequiredSkill identifier="cooking" level="25" />
        <RequiredItem identifier="he-preparredcrawlerfilets" amount="1" MinCondition="0.25" UseCondition="true" />
        <RequiredItem identifier="he-seaweedstrip" amount="1" />
        <RequiredItem identifier="he-emptyplate" amount="1" />
      </Fabricate>
      <Deconstruct time="4" requireddeconstructor="deconstructor">
        <Item identifier="he-dirtyplate" amount="1" />
      </Deconstruct>
      <Fabricate suitablefabricators="cafeteria" requiredtime="20" requiredmoney="49" fabricationlimitmin="0" fabricationlimitmax="10" />
      <Body width="60" radius="1" density="11" />
      <SuitableTreatment type="hunger" suitability="80" />
      <Holdable canbepicked="true" canBeCombined="false" removeOnCombined="false" slots="LeftHand,RightHand" holdangle="0" holdpos="36,-20" handle1="0,-4" handle2="0,-4" aimable="false" msg="ItemMsgPickUpSelect">
        <!-- EVERYTHING RELATED TO POISONING FOOD IS HANDLED VIA THE POISON EFFECT PRESET ITEMS DEFINED IN - variantof="" AND THEIR CODE IS LOCATED IN - "/Content/Items/#Presets/#ItemPresets.xml" -->
        
        <!--////////// HEALTH MENU USAGE [BOT] \\\\\\\\\\-->
        <!-- allows health menu usage on bots as long as they are not in water -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-20" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="18.0" />
          <Affliction identifier="he-stuffed" amount="1.25" />
          <ReduceAffliction type="damage" amount="0.4" />
          <ReduceAffliction type="hunger" amount="0.5" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Conditional trait_likes_filets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>

        <!--////////// HEALTH MENU USAGE [PLAYER] \\\\\\\\\\-->
        <!-- only allows health menu usage for players who are not in water and are unconcious/stunned -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-20" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="18.0" />
          <Affliction identifier="he-stuffed" amount="1.25" />
          <ReduceAffliction type="damage" amount="0.4" />
          <ReduceAffliction type="hunger" amount="0.5" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Conditional trait_likes_filets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>

        <!--////////// REGULAR EATING \\\\\\\\\\-->
        <!-- Use the item when it's held and the fork in the other hand is used (indicated by applying a "usingfork" tag on the character which this item checks for) -->
        <StatusEffect type="OnActive" target="This" Condition="-4.0" duration="1" stackable="false" comparison="And">
          <Conditional HasStatusTag="usingfork" targetcontainer="true" />
          <Conditional HasStatusTag="neq emptyfork" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" duration="1" stackable="false" comparison="And">
          <Conditional HasStatusTag="usingfork" />
          <Conditional HasStatusTag="neq emptyfork"/>
          <Conditional InWater="false" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="18.0" />
          <Affliction identifier="he-stuffed" amount="1.25" />
          <ReduceAffliction type="damage" amount="0.4" />
          <Affliction identifier="spill" amount="1000" probability="0.0005" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork1.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork2.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork3.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork4.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork5.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" duration="1" stackable="false">
          <Conditional HasStatusTag="usingfork" />
          <Conditional HasStatusTag="neq emptyfork" />
          <Conditional InWater="false" />
          <Conditional hunger="gt 0.5" />
          <ReduceAffliction type="hunger" amount="0.5" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" duration="1" stackable="false">
          <Conditional HasStatusTag="usingfork" />
          <Conditional HasStatusTag="neq emptyfork" />
          <Conditional InWater="false" />
          <Conditional trait_likes_filets="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>
        <!-- Prevent using this item continuously by applying the tag "emptyfork" which prevents the StatusEffect that uses the item from triggering again, effectively only allowing it to trigger once per mouse-click -->
        <StatusEffect type="OnActive" target="Character" tags="emptyfork" interval="0.1" duration="0.2">
          <Conditional HasStatusTag="usingfork" />
        </StatusEffect>
        <StatusEffect type="OnBroken" target="This" disabledeltatime="true">
          <Remove />
          <SpawnItem identifier="he-dirtyplate" spawnposition="SameInventory" spawnifcantbecontained="true" />
        </StatusEffect>
      </Holdable>
      <AiTarget sightrange="1000" static="true" />
    </Item>
    <!-- SOUPS & STEWS -->
    <Item hideinmenus="false" name="Craw Yum!" identifier="he-crawyumsoup" variantof="he-poisoneffectpreset_spoon" health="100" category="Misc" subcategory="Food" Tags="smallitem,fooditem,spoonfood" description="A soup." maxstacksize="1" maxstacksizecharacterinventory="1" cargocontaineridentifier="metalcrate" useinhealthinterface="false" scale="0.5" impactsoundtag="impact_bottle_glass" isshootable="false" requireaimtouse="false">
      <PreferredContainer secondary="wreckmedcab,abandonedmedcab" spawnprobability="0.02" />
      <PreferredContainer secondary="abandonedcrewcab" minamount="0" maxamount="1" spawnprobability="0.02" />
      <PreferredContainer secondary="crewcab" amount="1" spawnprobability="0.0" notcampaign="True"/>
      <PreferredContainer primary="table" secondary="kitchen,crewcab"/>
      <Sprite texture="%ModDir:2954998725%/Content/Items/Tools/Tools.png" sourcerect="342,0,45,20" depth="0.6" origin="0.5,0.5" />
      <!-- Gained from filling an empty bowl in a pot with soup
      <Fabricate suitablefabricators="stove" requiredtime="60" requiresrecipe="false" amount="1" displayname="crawlerfooditem">
        <RequiredSkill identifier="cooking" level="25" />
        <RequiredItem identifier="he-rawcrawlermeat" amount="1" MinCondition="0.5" MaxCondition="1.0" UseCondition="False" />
        <RequiredItem tag="water" UseCondition="True" MinCondition="0.1"/>
        <RequiredItem identifier="he-salt" UseCondition="True" MinCondition="0.05"/>
      </Fabricate> -->
      <Deconstruct time="1" requireddeconstructor="deconstructor"/>
      <Fabricate suitablefabricators="cafeteria" requiredtime="20" requiredmoney="49" fabricationlimitmin="0" fabricationlimitmax="10" displayname="crawlerfooditem" />
      <Body width="42" height="14" density="12" />
      <SuitableTreatment type="hunger" suitability="80" />
      <!-- <GreaterComponent canbeselected="false" canbepicked="false" allowingameediting="false" timeframe="0" /> -->
      <Holdable canbepicked="true" canBeCombined="false" removeOnCombined="false" slots="LeftHand,RightHand" holdangle="0" holdpos="36,-20" handle1="0,-4" handle2="0,-4" aimable="false" msg="ItemMsgPickUpSelect">
        <!-- EVERYTHING RELATED TO POISONING FOOD IS HANDLED VIA THE POISON EFFECT PRESET ITEMS DEFINED IN - variantof="" AND THEIR CODE IS LOCATED IN - "/Content/Items/#Presets/#ItemPresets.xml" -->
        
        <!--////////// HEALTH MENU USAGE [BOT] \\\\\\\\\\-->
        <!-- allows health menu usage on bots as long as they are not in water -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-20" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="24.0" />
          <Affliction identifier="he-stuffed" amount="0.75" />
          <ReduceAffliction type="nausea" amount="0.5" />
          <ReduceAffliction type="hunger" amount="0.25" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="false" />
          <Conditional trait_likes_soups="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>

        <!--////////// HEALTH MENU USAGE [PLAYER] \\\\\\\\\\-->
        <!-- only allows health menu usage for players who are not in water and are unconcious/stunned -->
        <StatusEffect type="OnSuccess" target="This,UseTarget" condition="-20" disabledeltatime="true" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="24.0" />
          <Affliction identifier="he-stuffed" amount="0.75" />
          <ReduceAffliction type="nausea" amount="0.5" />
          <ReduceAffliction type="hunger" amount="0.25" />
          <ReduceAffliction identifier="he-starvation" amount="10" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Drinks/Swallow1.ogg" volume="8.0" range="300" loop="false" />
        </StatusEffect>
        <StatusEffect type="OnSuccess" target="This,UseTarget" duration="5" comparison="And">
          <Conditional InWater="false" />
          <Conditional IsPlayer="true" />
          <Conditional stun="gt 0" />
          <Conditional trait_likes_soups="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>

        <!--////////// REGULAR EATING \\\\\\\\\\-->
        <!-- Use the item when it's held and the spoon in the other hand is used (indicated by applying a "usingspoon" tag on the character which this item checks for -->
        <StatusEffect type="OnActive" target="This" Condition="-4.0" duration="1" stackable="false" comparison="And">
          <Conditional HasStatusTag="usingspoon" targetcontainer="true" />
          <Conditional HasStatusTag="neq emptyspoon" targetcontainer="true" />
          <Conditional InWater="false" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" duration="1" stackable="false" comparison="And">
          <Conditional HasStatusTag="usingspoon" />
          <Conditional HasStatusTag="neq emptyspoon"/>
          <Conditional InWater="false" />
          <Affliction identifier="he-increasedswimmingspeedcrawler" amount="24.0" />
          <Affliction identifier="he-stuffed" amount="0.75" />
          <ReduceAffliction type="nausea" amount="0.5" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork1.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork2.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork3.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork4.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
          <Sound file="%ModDir:2954998725%/Content/Sounds/Items/Food/Fork5.ogg" loop="False" range="200" volume="5.0" selectionmode="Random" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" duration="1" stackable="false">
          <Conditional HasStatusTag="usingspoon" />
          <Conditional HasStatusTag="neq emptyspoon" />
          <Conditional InWater="false" />
          <Conditional hunger="gt 0.25" />
          <ReduceAffliction type="hunger" amount="0.25" />
        </StatusEffect>
        <StatusEffect type="OnSecondaryUse" target="Character" comparison="And" duration="1" stackable="false">
          <Conditional HasStatusTag="usingspoon" />
          <Conditional HasStatusTag="neq emptyspoon" />
          <Conditional InWater="false" />
          <Conditional trait_likes_soups="gt 0" />
          <ReduceAffliction identifier="mood" amount="0.5" />
        </StatusEffect>
        <!-- Prevent using this item continuously by applying the tag "emptyspoon" which prevents the StatusEffect that uses the item from triggering again, effectively only allowing it to trigger once per mouse-click -->
        <StatusEffect type="OnActive" target="Character" tags="emptyspoon" interval="0.1" duration="0.2">
          <Conditional HasStatusTag="usingspoon" />
        </StatusEffect>
        <StatusEffect type="OnBroken" target="This" disabledeltatime="true">
          <Remove />
          <SpawnItem identifier="he-dirtybowl" spawnposition="SameInventory" spawnifcantbecontained="true" />
        </StatusEffect>
      </Holdable>
      <AiTarget sightrange="1000" static="true" />
    </Item>
  </Items>
</Override>