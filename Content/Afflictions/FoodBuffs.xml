  <!-- 
    NEW GENERAL FOOD BUFFS (WIP):
    merges effect into these
            
    - "Calcium Rich"          (blunt+stuffed resistance)         cheese, mudraptor meat
    - "Lean fats"            (swim speed)                        crawler, spineling
    - "Protein Rich"         (melee damage,weapons skill)        hammerhead, cheese
    - "Vitamin Rich"         (O2 capacity, bloodloss reduction)  threshers, salads 
    - "Sugar Rich"          (skill learning, fatigue res)        sugary meals
    - "Fiber Rich"          (defecation, stomachache res)        vegetables
       
    NOTE: if you want a food item to reach the 1800% cap of foodbuffs, use a 20/1  foodbuff/stuffed ratio
          stuffed makes you vomit above 80, so you want to gain 1800 foodbuff at 80 stuffed
          when hunger is factored (allows you to stack more foodbuff before reaching stuffed), then 20 is a good number
          Only the BEST foods should make you cap before getting fully stuffed
          
    
    like this:    
          <Affliction identifier="he-proteinrich" amount="20" />
          <Affliction identifier="he-stuffed" amount="1" />
       
       All foodbuff effects ramp up from 0-100 and then have their full effect above 100
       
    --> 
  
  
    <!-- Calcium rich diet, gives resistance to bone damage and helps digesting -->
    <Affliction name="Calcium Rich Diet"
      identifier="he-calciumrich"
      type="foodbuff"
      isbuff="true"
      limbspecific="false"
      indicatorlimb="Torso"
      hideiconafterdelay="true"
      showiconthreshold="100"
      showinhealthscannerthreshold="100"
      treatmentthreshold="100"
      maxstrength="1800"
      iconcolors="150,255,150;75,255,75;0,255,0"
      MedicalSkillGain="0.0">
      <Effect minstrength="0" maxstrength="100" strengthchange="-1"
        resistancefor="blunttrauma"
        minresistance="0.0"
        maxresistance="0.25">
        <StatusEffect target="Character" >
          <ReduceAffliction identifier="he-stuffed" strength="0.010" />
        </StatusEffect>
      </Effect>
      <Effect minstrength="100" maxstrength="1800" strengthchange="-1"
        resistancefor="blunttrauma"
        minresistance="0.25"
        maxresistance="0.25" >
        <StatusEffect target="Character" >
          <ReduceAffliction identifier="he-stuffed" strength="0.025" />
        </StatusEffect>
      </Effect>
      <icon texture="%ModDir:2954998725%/Content/UI/HE-IconsAtlas.png" sourcerect="909,0,115,123" color="40,255,20,255" origin="0.5,0.5" />
    </Affliction>
    
