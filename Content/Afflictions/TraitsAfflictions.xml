<?xml version="1.0" encoding="utf-8" ?>
<!-- These are hidden afflictions that do not heal over time and are obtained by enabling mood on a character. They're checked while eating or drinking to increase/decrease mood -->
<Override>
  <Afflictions>
    <!-- FOOD -->    

    <Affliction name="" identifier="trait_likes_friedfood" description="" type="trait" limbspecific="false" indicatorlimb="Head" maxstrength="1" affectmachines="false" showiconthreshold="1000" showicontoothersthreshold="1000" showinhealthscannerthreshold="1000" healableinmedicalclinic="false">
      <Effect minstrength="0" maxstrength="1" strengthchange="0" resistancefor="trait_dislikes_friedfood" minresistance="1" maxresistance="1" multiplybymaxvitality="true" dialogflag="likesfriedfood">
        <StatusEffect target="Character">
          <Conditional ate_friedfood="gt 0" />
          <ReduceAffliction identifier="mood" amount="1.0" />
        </StatusEffect> 
      </Effect>
    </Affliction>
    <Affliction name="" identifier="ate_friedfood" description="" type="trait" limbspecific="false" indicatorlimb="Head" maxstrength="1" affectmachines="false" showiconthreshold="1000" showicontoothersthreshold="1000" showinhealthscannerthreshold="1000" healableinmedicalclinic="false">
      <Effect minstrength="0" maxstrength="10" strengthchange="-1" resistancefor="ate_friedfood" minresistance="1" maxresistance="1" multiplybymaxvitality="true" />
